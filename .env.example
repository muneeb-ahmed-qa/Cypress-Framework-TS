# Environment Variables Template
# Copy this file to .env and update with your actual values

# Application URLs
CYPRESS_BASE_URL=https://example.cypress.io
CYPRESS_API_URL=https://jsonplaceholder.typicode.com

# Environment
CYPRESS_ENVIRONMENT=staging

# Encrypted Passwords (use encryptPassword utility to generate)
# Format: encrypted_password|salt
ENCRYPTED_ADMIN_PASSWORD=encrypted_admin_password_here
ENCRYPTED_USER_PASSWORD=encrypted_user_password_here
ENCRYPTED_TEST_PASSWORD=encrypted_test_password_here

# Encryption Key (keep this secret!)
ENCRYPTION_KEY=your_secret_encryption_key_here

# API Keys (if needed)
API_KEY=your_api_key_here

# Database (if needed)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=test_db
