{
  "name": "cypress-advanced-framework",
  "version": "1.0.0",
  "description": "Advanced Cypress Testing Framework showcasing professional automation skills",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "cypress:run:headless": "cypress run --headed false",
    "test:api": "cypress run --env TAGS=api",
    "test:e2e": "cypress run --spec 'cypress/e2e/ui/**/*.spec.ts'",
    "test:visual": "cypress run --env TAGS=visual",
    "merge:reports": "mochawesome-merge cypress/reports/*.json -o cypress/reports/report.json",
    "generate:report": "marge cypress/reports/report.json --reportDir cypress/reports/html --inline",
    "report": "npm run merge:reports && npm run generate:report",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "encrypt-passwords": "node scripts/encrypt-passwords.js",
    "setup-env": "cp .env.example .env && echo 'Please update .env with your actual values'",
    "generate-data": "node scripts/generate-test-data.js",
    "generate-users": "node scripts/generate-test-data.js user --count 10 --output users.json",
    "generate-products": "node scripts/generate-test-data.js product --count 20 --output products.json",
    "generate-orders": "node scripts/generate-test-data.js order --count 15 --output orders.json",
    "test:smoke": "cypress run --env TAGS=smoke",
    "test:regression": "cypress run --env TAGS=regression",
    "test:critical": "cypress run --env TAGS=critical",
    "test:ui": "cypress run --env TAGS=ui",
    "test:fast": "cypress run --env EXCLUDE_TAGS=slow,performance",
    "test:stable": "cypress run --env EXCLUDE_TAGS=flaky,unstable",
    "test:integration": "cypress run --env TAGS=integration,e2e",
    "test:performance": "cypress run --env TAGS=performance,slow",
    "test:custom": "cypress run --env TAGS",
    "test": "cypress run",
    "test:tagged": "node scripts/run-tagged-tests.js",
    "test:list-tags": "node scripts/run-tagged-tests.js --list-tags",
    "test:html-report": "cypress run --reporter cypress-mochawesome-reporter --reporter-options reportDir=cypress/reports/html,charts=true,reportPageTitle='Cypress Test Report',embeddedScreenshots=true,inlineAssets=true",
    "test:junit-report": "cypress run --reporter mocha-junit-reporter --reporter-options mochaFile=cypress/reports/junit/results.xml,toConsole=true,attachments=true",
    "test:multi-report": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=cypress/reports/multi-reporter.json",
    "test:ci": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=cypress/reports/ci-reporter.json",
    "report:merge": "mochawesome-merge cypress/reports/json/*.json -o cypress/reports/merged-report.json",
    "report:generate": "marge cypress/reports/merged-report.json --reportDir cypress/reports/html --inline",
    "report:all": "npm run report:merge && npm run report:generate",
    "report:clean": "rm -rf cypress/reports/html/* cypress/reports/junit/* cypress/reports/json/*",
    "report:generate-script": "node scripts/generate-reports.js",
    "report:generate-html": "node scripts/generate-reports.js --type html",
    "report:generate-all": "node scripts/generate-reports.js --type all --clean"
  },
  "keywords": [
    "cypress",
    "testing",
    "automation",
    "e2e",
    "api-testing"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.11",
    "@types/cypress": "^1.1.3",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "axios": "^1.6.2",
    "cypress": "^13.6.1",
    "cypress-axe": "^1.5.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-junit-reporter": "^1.3.1",
    "cypress-mochawesome-reporter": "^3.7.0",
    "cypress-multi-reporters": "^1.6.3",
    "cypress-real-events": "^1.11.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^3.1.1",
    "typescript": "^5.9.3"
  }
}
